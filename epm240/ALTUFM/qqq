#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 3;
:vpi_module "/usr/lib/i386-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/i386-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/i386-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/i386-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/i386-linux-gnu/ivl/va_math.vpi";
S_0x864b10 .scope module, "test_counter" "test_counter" 2 2;
 .timescale -3 -3;
v0x874e60_0 .var "clk", 0 0;
v0x890f50_0 .net "led1", 0 0, v0x874d10_0;  1 drivers
v0x890fb0_0 .net "led2", 0 0, v0x874d80_0;  1 drivers
S_0x864bf0 .scope module, "counter_inst" "counter" 2 8, 3 1 0, S_0x864b10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 1 "led1";
    .port_info 2 /OUTPUT 1 "led2";
P_0x864cd0 .param/l "PERIODLED" 0 3 3, +C4<00000000000000000000000000000010>;
v0x864dc0_0 .net "clk", 0 0, v0x874e60_0;  1 drivers
v0x874c20_0 .var/i "count", 31 0;
v0x874ca0_0 .var/i "countone", 31 0;
v0x874d10_0 .var "led1", 0 0;
v0x874d80_0 .var "led2", 0 0;
E_0x864d90 .event posedge, v0x864dc0_0;
    .scope S_0x864bf0;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x874ca0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x874c20_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x864bf0;
T_1 ;
    %wait E_0x864d90;
    %load/vec4 v0x874ca0_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz  T_1.0, 5;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x874d10_0, 0;
T_1.0 ;
    %pushi/vec4 5, 0, 32;
    %load/vec4 v0x874ca0_0;
    %cmp/s;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %load/vec4 v0x874ca0_0;
    %cmpi/s 10, 0, 32;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x874d10_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x874d10_0, 0;
T_1.3 ;
    %load/vec4 v0x874ca0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x874ca0_0, 0, 32;
    %load/vec4 v0x874c20_0;
    %cmpi/s 2, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz  T_1.4, 5;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x874d80_0, 0;
    %load/vec4 v0x874c20_0;
    %addi 1, 0, 32;
    %store/vec4 v0x874c20_0, 0, 32;
    %jmp T_1.5;
T_1.4 ;
    %pushi/vec4 2, 0, 32;
    %load/vec4 v0x874c20_0;
    %cmp/s;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %load/vec4 v0x874c20_0;
    %cmpi/s 4, 0, 32;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.6, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x874d80_0, 0;
    %load/vec4 v0x874c20_0;
    %addi 1, 0, 32;
    %store/vec4 v0x874c20_0, 0, 32;
    %jmp T_1.7;
T_1.6 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x874c20_0, 0, 32;
T_1.7 ;
T_1.5 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x864b10;
T_2 ;
    %delay 10, 0;
    %load/vec4 v0x874e60_0;
    %inv;
    %store/vec4 v0x874e60_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x864b10;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x874e60_0, 0, 1;
    %delay 50, 0;
    %vpi_call 2 18 "$display", "Running---%d", $time {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x864b10;
T_4 ;
    %delay 400, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x864b10;
T_5 ;
    %vpi_call 2 31 "$dumpfile", "out.vcd" {0 0 0};
    %vpi_call 2 32 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x864b10 {0 0 0};
    %end;
    .thread T_5;
    .scope S_0x864b10;
T_6 ;
    %vpi_call 2 37 "$monitor", $stime, " ", " ", v0x874e60_0, " ", " ", v0x890f50_0, " ", " ", v0x890fb0_0 {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tcounter.v";
    "counter.v";
